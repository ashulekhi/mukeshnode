var express = require('express');

var router = express.Router();
var ashuController = require('./ashu.controller');

router.get('/' , ashuController)
router.get('/getData' , ashuController.getData);
router.get('/getImages' , ashuController.fetchImages)
router.post('/update' , ashuController.fetchImages)




module.exports = router;   //// We exportes the router